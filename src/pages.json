{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/login/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/historyError/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/photoCheckCar/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/historyCheckCar/index",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "easycom": {
    "custom": {
      "^u-(.*)": "vk-uview-ui/components/u-$1/u-$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#333333",
    "backgroundColor": "#fff",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "12px",
    "spacing": "5px",
    "iconWidth": "22px",
    "list": [
      {
        "pagePath": "pages/historyError/index",
        "text": "查看历史异常记录",
        "iconPath": "/static/newHome/home.png",
        "selectedIconPath": "/static/newHome/home_active.png"
      },
      {
        "pagePath": "pages/photoCheckCar/index",
        "text": "拍照检车作业",
        "iconPath": "/static/newHome/message.png",
        "selectedIconPath": "/static/newHome/message_active.png"
      },
      {
        "pagePath": "pages/historyCheckCar/index",
        "text": "查看历史检车记录",
        "iconPath": "/static/newHome/mine.png",
        "selectedIconPath": "/static/newHome/mine_active.png"
      }
    ]
  }
}
